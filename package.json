{
  "name": "node-red-contrib-ical-events",
  "version": "0.12.2",
  "description": "NodeRed iCal Adapter",
  "author": {
    "name": "naimo84",
    "email": "git@neumann-benjamin.de"
  },
  "contributors": [
    {
      "name": "naimo84",
      "email": "git@neumann-benjamin.de"
    }
  ],
  "homepage": "https://github.com/naimo84/node-red-contrib-ical-events",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/naimo84/node-red-contrib-ical-events/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "node-red",
    "ical",
    "calendar",
    "caldav",
    "ics",
    "Smart Home",
    "home automation"
  ],
  "node-red": {
    "nodes": {
      "ical-config": "dist/ical-config.js",
      "ical-events": "dist/ical-events.js",
      "ical-sensor": "dist/ical-sensor.js",
      "ical-upcoming": "dist/ical-upcoming.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naimo84/node-red-contrib-ical-events"
  },
  "scripts": {
    "start": "node-red",
    "test": "mocha \"test/**/*_spec.js\"",
    "release": "standard-version",
    "release-minor": "standard-version --release-as minor",
    "watch": "tsc -w -p ./tsconfig.json",
    "dev": "gulp develop",
    "build": "gulp"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "2500ms",
    "verbose": true,
    "ext": "js,html"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit"
    }
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/NeumannBenjamin"
  },
  "dependencies": {
    "cloneextend": "0.0.3",
    "cron": "^1.7.2",
    "cron-parser": "^2.13.0",
    "crypto-js": "^3.1.9-1",
    "dav": "^1.8.0",
    "https": "^1.0.0",
    "ical-expander": "2.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "node-cache": "^5.1.0",
    "node-ical": "^0.10.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/cron": "^1.7.1",
    "@types/crypto-js": "^3.1.43",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^13.1.8",
    "@types/node-red": "^0.20.1",
    "chai": "4.2.0",
    "chai-like": "1.1.1",
    "chai-things": "0.2.0",
    "commitlint": "^8.3.5",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-watch": "^5.0.1",
    "husky": "4.0.10",
    "mocha": "7.0.0",
    "node-red": "1.0.3",
    "node-red-node-test-helper": "^0.2.3",
    "nodemon": "^1.19.4",
    "sinon": "8.1.0",
    "standard-version": "^7.1.0",
    "typescript": "^3.7.5"
  },
  "commitlint": {
    "format": {
      "helpUrl": "test"
    },
    "rules": {
      "body-leading-blank": [
        1,
        "always"
      ],
      "footer-leading-blank": [
        1,
        "always"
      ],
      "header-max-length": [
        2,
        "always",
        72
      ],
      "scope-case": [
        2,
        "always",
        "lower-case"
      ],
      "subject-case": [
        2,
        "never",
        [
          "sentence-case",
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  }
}
